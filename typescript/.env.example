# Temporal Configuration
TEMPORAL_ADDRESS=localhost:7233
TEMPORAL_TLS=false
TEMPORAL_API_KEY=
TEMPORAL_NAMESPACE=default

# LLM Provider Configuration
# Options: "openai" or "lmstudio"
LLM_PROVIDER=openai

# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
# Optional: Custom OpenAI base URL (for proxies or custom endpoints)
OPENAI_BASE_URL=

# LM Studio Configuration
LMSTUDIO_BASE_URL=http://localhost:1234/v1
# Optional: API key if your LM Studio setup requires it
LMSTUDIO_API_KEY=

# Model Configuration
# For OpenAI: gpt-4o, gpt-4o-mini, gpt-3.5-turbo, etc.
# For LM Studio: depends on your loaded model
DEFAULT_MODEL=gpt-4o-mini